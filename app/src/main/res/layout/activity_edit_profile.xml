<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/edit_text_border"
    tools:context=".EditProfileActivity">

    <TextView
        android:id="@+id/txtProfileName"
        android:layout_width="58dp"
        android:layout_height="23dp"
        android:layout_marginTop="128dp"
        android:layout_marginEnd="40dp"
        android:text="Name"
        android:textColor="#000000"
        app:layout_constraintEnd_toStartOf="@+id/editProfileName"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtProfilePhone"
        android:layout_width="107dp"
        android:layout_height="24dp"
        android:layout_marginTop="208dp"
        android:layout_marginEnd="40dp"
        android:text="Phone Number"
        android:textColor="#000000"
        app:layout_constraintEnd_toStartOf="@+id/editProfilePhone"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtProfileEmail"
        android:layout_width="110dp"
        android:layout_height="19dp"
        android:layout_marginTop="288dp"
        android:layout_marginEnd="40dp"
        android:text="E-Mail Address"
        android:textColor="#000000"
        app:layout_constraintEnd_toStartOf="@+id/editProfileEmail"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtProfileBirth"
        android:layout_width="90dp"
        android:layout_height="26dp"
        android:layout_marginTop="368dp"
        android:layout_marginEnd="40dp"
        android:text="Date of Birth"
        android:textColor="#000000"
        app:layout_constraintEnd_toStartOf="@+id/editProfileBirth"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtProfilePassword"
        android:layout_width="69dp"
        android:layout_height="24dp"
        android:layout_marginTop="464dp"
        android:layout_marginEnd="48dp"
        android:text="Password"
        android:textColor="#000000"
        app:layout_constraintEnd_toStartOf="@+id/btnProfileChangePassword"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editProfileName"
        android:layout_width="196dp"
        android:layout_height="46dp"
        android:layout_marginTop="116dp"
        android:layout_marginEnd="36dp"
        android:background="@drawable/edit_text_border"
        android:ems="10"
        android:inputType="textPersonName"
        android:textAlignment="viewEnd"
        android:textColor="#000000"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editProfilePhone"
        android:layout_width="200dp"
        android:layout_height="46dp"
        android:layout_marginTop="196dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/edit_text_border"
        android:ems="10"
        android:inputType="textPersonName"
        android:textAlignment="viewEnd"
        android:textColor="#000000"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editProfileEmail"
        android:layout_width="196dp"
        android:layout_height="46dp"
        android:layout_marginTop="276dp"
        android:layout_marginEnd="36dp"
        android:background="@drawable/edit_text_border"
        android:ems="10"
        android:inputType="textPersonName"
        android:textAlignment="viewEnd"
        android:textColor="#000000"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editProfileBirth"
        android:layout_width="196dp"
        android:layout_height="46dp"
        android:layout_marginTop="356dp"
        android:layout_marginEnd="36dp"
        android:background="@drawable/edit_text_border"
        android:ems="10"
        android:inputType="textPersonName"
        android:textAlignment="viewEnd"
        android:textColor="#000000"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnProfileChangePassword"
        android:layout_width="190dp"
        android:layout_height="34dp"
        android:layout_marginTop="452dp"
        android:layout_marginEnd="40dp"
        android:background="@drawable/edit_text_border"
        android:text="Change Password"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnUpdateProfile"
        android:layout_width="182dp"
        android:layout_height="43dp"
        android:layout_marginStart="120dp"
        android:layout_marginBottom="64dp"
        android:background="@drawable/edit_text_border"
        android:text="UPDATE PROFILE"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/btnCloseEditProfileActivity"
        android:layout_width="20dp"
        android:layout_height="23dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/circleunfilled"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.112"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_baseline_cancel_24" />

    <TextView
        android:id="@+id/txtTitleProfile"
        android:layout_width="116dp"
        android:layout_height="20dp"
        android:layout_marginEnd="220dp"
        android:text="User Profile"
        android:textAlignment="center"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.067" />

</androidx.constraintlayout.widget.ConstraintLayout>